#!/usr/bin/expect

if {$argc < 3} {
    puts "Usage:cmd <rhost> <remoteusername> <remotepassword>"
    exit 1
}

set timeout -1
set rhost [lindex $argv 0] 
set rusername [lindex $argv 1]
set rpassword [lindex $argv 2]

spawn ssh-copy-id -i /root/.ssh/id_rsa.pub $rusername@$rhost
expect "*password*" 
send "$rpassword\r"

interact
